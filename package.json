{
  "name": "letslearn",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start:dev": "ng serve --configuration=production --watch --disable-host-check --base-href / --host 0.0.0.0 --port 4200",
    "test": "ng test",
    "test:phantom": "ng test --karmaConfig=src/karma.phantom.conf.js",
    "test:dev": "ng test --watch --karmaConfig=src/karma.dev.conf.js",
    "test:puppeteer": "ng test --karmaConfig=src/karma.puppeteer.conf.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier:check": "prettier --list-different \"src/**/*.ts\"",
    "stylelint": "stylelint \"src/**/*.css\""
  },
  "lint-staged": {
    "*.scss": [
      "node_modules/.bin/prettier --write --config .prettierrc",
      "stylelint",
      "git add"
    ],
    "*.ts": [
      "node_modules/.bin/prettier --write --config .prettierrc",
      "tslint",
      "git add"
    ],
    "*.html": [
      "node_modules/.bin/js-beautify -r",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "intl": "^1.2.5",
    "rxjs": "~6.3.3",
    "save-dev": "0.0.1-security",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.9",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "husky": "^4.2.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "prettier": "^2.0.5",
    "prettify-xml": "^1.2.0",
    "protractor": "~5.4.0",
    "surge": "^0.21.3",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2"
  }
}
